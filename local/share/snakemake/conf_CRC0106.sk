include: "../conf.sk"

SAMPLES=["CRC0106LMX0B01201TUMD08000","CRC0106LMX0B02201TUMD02000","CRC0106LMX0B02202TUMD02000","CRC0106LMX0B03202TUMD05000","CRC0106LMX0B03206TUMD06000","CRC0106LMX0B04003TUMD02000","CRC0106LMX0B05042TUMD07000"]

REF=SAMPLES[0]
SAMPLESD=SAMPLES[1:len(SAMPLES)]
SAMPS=",".join(SAMPLESD)

rule all_something:
    input: expand("{sample}.{{something}}", sample=SAMPLES)
    output: "all_samples_{something}"
    shell: "touch {output}"
